    "express": "^4.16.3",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "pug": "^2.0.3",
    "mysql": "^2.16.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "github:types/mysql",
    "@types/jsonwebtoken": "^7.2.8",
